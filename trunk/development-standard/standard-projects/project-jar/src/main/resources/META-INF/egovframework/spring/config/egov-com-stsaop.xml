	<!-- 로거를 bean 으로 셋팅 -->
    <bean id="logAspect" class="egovframework.com.ext.jstree.support.manager.aop.web.LoggingAspect"/>
	 
    <!-- 로거 포인트컷 설정 -->
    <aop:config>
        <aop:aspect id="aspectLogggingBefore" ref="logAspect">
 
            <!-- @Before -->
            <aop:before method="logBefore" pointcut="execution(* egovframework.com.ext.jstree.springiBatis..*(..))"/>
        </aop:aspect>
        <aop:aspect id="aspectLogggingAfter" ref="logAspect">
            <!-- @After -->
            <aop:after method="logAfter" pointcut="execution(* egovframework.com.ext.jstree.springiBatis..*(..))"/>
        </aop:aspect>

    </aop:config>
</beans>