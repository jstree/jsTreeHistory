	<!--  웹로그 인터셉터 End -->
	
	<bean id="localeChangeInterceptor"
		class="org.springframework.web.servlet.i18n.LocaleChangeInterceptor">
		<property name="paramName" value="lang" />
	</bean>

	<!-- interceptor -->
	<bean id="requestHandlerInterceptor"
		class="egovframework.com.ext.jstree.support.manager.interceptor.RequestHandlerInterceptor">
	</bean>
	
	
	<mvc:interceptors> 
         <ref bean="localeChangeInterceptor"/>
		<mvc:interceptor>   
        	<mvc:mapping path="/**"/> 
		
         <ref bean="requestHandlerInterceptor"/>
		</mvc:interceptor>
	</mvc:interceptors>

</beans>
