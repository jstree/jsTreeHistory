<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:dwr="http://www.directwebremoting.org/schema/spring-dwr" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
       http://www.directwebremoting.org/schema/spring-dwr http://www.directwebremoting.org/schema/spring-dwr-3.0.xsd">

	<dwr:configuration>
		<dwr:convert type="bean" class="egovframework.com.ext.jstree.springDWR.domain.User"></dwr:convert>
	</dwr:configuration>
	<dwr:annotation-config id="dwrAnnotationConfig" />
	<dwr:annotation-scan base-package="egovframework.com.ext.jstree.springDWR.*.*" scanDataTransferObject="true" scanRemoteProxy="true" />
</beans>